{
  "name": "webpack-tutorial",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "cleanup": "npx rimraf ./public",
    "dev": "yarn run webpack:dev && yarn run webpack:server",
    "webpack:server": "npx webpack-dev-server --config ./webpack.dev.js",
